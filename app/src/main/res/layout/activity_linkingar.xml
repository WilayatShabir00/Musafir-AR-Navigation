<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Linkingar">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="282dp"
        android:layout_height="215dp"
        android:onClick="Clicked1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.271"
        app:srcCompat="@drawable/gpstarck" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="371dp"
        android:layout_height="77dp"
        android:layout_marginTop="32dp"
        android:fontFamily="@font/aclonica"
        android:text="If You Want To Track your friend have a chat with him. "
        android:textColor="#173502"
        android:textColorHint="#873030"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.315"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="359dp"
        android:layout_height="64dp"
        android:layout_marginTop="48dp"
        android:fontFamily="@font/aclonica"
        android:text="If you want to reach a destination with Ar."
        android:textColor="#073A09"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.296"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="288dp"
        android:layout_height="207dp"
        android:layout_marginTop="8dp"
        android:onClick="Clicked2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.355"
        app:srcCompat="@drawable/ar" />
</androidx.constraintlayout.widget.ConstraintLayout>